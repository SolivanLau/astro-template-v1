{
    "name": "astro-template-v1",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "dotenvx run -f .env.development -- astro dev",
        "check": "dotenvx run -- astro check",
        "start": "dotenvx run -- astro dev",
        "build": "astro check && dotenvx run -- astro build",
        "preview": "dotenvx run -- astro preview",
        "astro": "astro",
        "format": "pnpm exec prettier . --write"
    },
    "dependencies": {
        "@astrojs/check": "^0.9.2",
        "@astrojs/netlify": "^5.5.0",
        "@astrojs/partytown": "^2.1.1",
        "@dotenvx/dotenvx": "^1.14.1",
        "astro": "^4.13.2",
        "astro-icon": "^1.1.0",
        "qs": "^6.13.0",
        "sharp": "^0.33.4"
    },
    "devDependencies": {
        "@types/node": "^22.1.0",
        "@types/qs": "^6.9.15",
        "prettier": "^3.3.3",
        "prettier-plugin-astro": "^0.14.1",
        "sass": "^1.77.8"
    }
}
